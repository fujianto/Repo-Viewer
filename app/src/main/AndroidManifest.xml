<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.naisinpo.fujianto.repoviewer" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/github_icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.naisinpo.fujianto.repoviewer.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.naisinpo.fujianto.repoviewer.DetailActivity"
            android:label="@string/title_activity_detail"
            android:parentActivityName="com.naisinpo.fujianto.repoviewer.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.naisinpo.fujianto.repoviewer.MainActivity" />
        </activity>
        <activity
            android:name="com.naisinpo.fujianto.repoviewer.SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName="com.naisinpo.fujianto.repoviewer.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.naisinpo.fujianto.repoviewer.MainActivity" />
        </activity>
    </application>

</manifest>
